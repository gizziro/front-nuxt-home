{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "debug:env": "dotenv -e .env.prd -- node -e \"console.log(process.env)\"",
    "build": "nuxi build",
    "build:prod": "dotenv -e .env.prd -- nuxi build",
    "dev": "nuxi dev --host",
    "dev:local": "dotenv -e .env.local -- nuxi dev --host",
    "dev:test": "dotenv -e .env.test -- nuxi dev --host",
    "generate": "nuxi generate",
    "preview": "nuxi preview",
    "preview:prod": "dotenv -e .env.prd -- nuxi preview",
    "postinstall": "nuxi prepare",
    "start": "node .output/server/index.mjs"
  },
  "dependencies": {
    "@nuxt/eslint": "^1.3.0",
    "@nuxt/icon": "^1.12.0",
    "@nuxt/scripts": "^0.11.6",
    "@pinia/nuxt": "^0.11.0",
    "@primeuix/themes": "^1.1.1",
    "@primevue/forms": "^4.3.4",
    "@primevue/nuxt-module": "^4.3.4",
    "@tailwindcss/vite": "^4.1.6",
    "eslint": "^9.26.0",
    "nuxt": "^3.17.2",
    "pinia": "^3.0.2",
    "pinia-plugin-persistedstate": "^4.3.0",
    "primeicons": "^7.0.0",
    "primevue": "^4.3.4",
    "tailwindcss": "^4.1.6",
    "unenv": "^1.10.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.1",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "dotenv-cli": "^8.0.0"
  }
}
